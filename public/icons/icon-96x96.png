<svg width="512" height="512" viewBox="0 0 512 512" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle -->
  <circle cx="256" cy="256" r="256" fill="#3B82F6"/>
  
  <!-- Airplane shape -->
  <g transform="translate(256, 256)">
    <!-- Main body -->
    <rect x="-8" y="-80" width="16" height="160" rx="8" fill="white"/>
    
    <!-- Wings -->
    <rect x="-120" y="-20" width="240" height="24" rx="12" fill="white"/>
    
    <!-- Tail wings -->
    <rect x="-40" y="60" width="80" height="16" rx="8" fill="white"/>
    
    <!-- Nose -->
    <circle cx="0" cy="-88" r="8" fill="white"/>
    
    <!-- Wing details -->
    <rect x="-100" y="-16" width="200" height="4" rx="2" fill="#3B82F6"/>
    <rect x="-80" y="-12" width="160" height="4" rx="2" fill="#3B82F6"/>
    
    <!-- Body line -->
    <rect x="-2" y="-72" width="4" height="144" rx="2" fill="#3B82F6"/>
    
    <!-- Tail details -->
    <rect x="-30" y="64" width="60" height="4" rx="2" fill="#3B82F6"/>
    
    <!-- Cockpit -->
    <ellipse cx="0" cy="-40" rx="12" ry="20" fill="#1E40AF"/>
    
    <!-- Wing tips -->
    <circle cx="-120" cy="-8" r="6" fill="#1E40AF"/>
    <circle cx="120" cy="-8" r="6" fill="#1E40AF"/>
  </g>
  
  <!-- Motion lines for dynamic effect -->
  <g opacity="0.3">
    <rect x="80" y="230" width="60" height="2" rx="1" fill="white"/>
    <rect x="90" y="240" width="40" height="2" rx="1" fill="white"/>
    <rect x="70" y="250" width="50" height="2" rx="1" fill="white"/>
    <rect x="85" y="260" width="35" height="2" rx="1" fill="white"/>
    
    <rect x="372" y="230" width="60" height="2" rx="1" fill="white"/>
    <rect x="382" y="240" width="40" height="2" rx="1" fill="white"/>
    <rect x="392" y="250" width="50" height="2" rx="1" fill="white"/>
    <rect x="387" y="260" width="35" height="2" rx="1" fill="white"/>
  </g>
</svg>